<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="FormsDS" transaction-type="JTA">
		<class>com.forms.server.entities.Appuser</class>
		<class>com.forms.server.entities.District</class>
		<class>com.forms.server.entities.FinancialYear</class>
		<class>com.forms.server.entities.Form</class>
		<class>com.forms.server.entities.Question</class>
		<class>com.forms.server.entities.Section</class>
		<class>com.forms.server.entities.SectionHasQuestion</class>
		<class>com.forms.server.entities.SectionHasQuestionPK</class>
		<class>com.forms.server.entities.SocietyName</class>
		<properties>
			<property name="hibernate.connection.username" value="root"/>
			<property name="hibernate.connection.password" value="root"/>
			<property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/forms_001"/>
			<property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
			<property name="hibernate.show_sql" value="true"/>
		</properties>
	</persistence-unit>
</persistence>
